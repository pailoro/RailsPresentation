<div class="m-login__signin">
  <div class="m-login__head">
    <h3 class="m-login__title">
      Entrar na Ã€rea Administrativa
    </h3>
  </div>
  <%= form_for(resource, as: resource_name, url: session_path(resource_name), html: { class: "m-login__form m-form" }) do |f| %>
    <div class="form-group m-form__group">
      <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "form-control m-input",  placeholder: "Email"  %>
    </div>
    <div class="form-group m-form__group">
      <%= f.password_field :password, autocomplete: "current-password", class: "form-control m-input m-login__form-input--last", placeholder: "Senha" %>
    </div>
    <div class="row m-login__form-sub">
      <div class="col m--align-left m-login__form-left">
        <label class="m-checkbox  m-checkbox--focus">
          <%= f.check_box :remember_me %>
          Lembrar-me
          <span></span>
        </label>
      </div>
      <div class="col m--align-right m-login__form-right">
        <a href="/admin/esqueci-a-senha" id="m_login_forget_password" class="m-link">
          Esqueceu a senha?
        </a>
      </div>
    </div>
    <div class="m-login__form-action">

      <%= f.submit "Entrar", class: "btn btn-focus m-btn m-btn--pill m-btn--custom m-btn--air m-login__btn m-login__btn--primary" %>
    </div>
  <% end %>
</div>

