<!DOCTYPE html>
<html lang="en" >
	<!-- begin::Head -->
	<head>
		<meta charset="utf-8" />
		<title>
			Beep App
		</title>
		<%= csrf_meta_tags %>
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
		<!--begin::Web font -->
		<script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.16/webfont.js"></script>
		<script>
          WebFont.load({
            google: {"families":["Poppins:300,400,500,600,700","Roboto:300,400,500,600,700"]},
            active: function() {
                sessionStorage.fonts = true;
            }
          });
		</script>
		<!--end::Web font -->
        <!--begin::Base Styles -->
		<%= stylesheet_link_tag "application", "admin/vendors/base/vendors.bundle.css", "admin/demo/default/base/style.bundle.css", 'application.css', 'data-turbolinks-track': 'reload' %>
		<!--end::Base Styles -->


	</head>
	<!-- end::Head -->
    <!-- end::Body -->
	<body  class="m--skin- m-header--fixed m-header--fixed-mobile m-aside-left--enabled m-aside-left--skin-dark m-aside-left--offcanvas m-footer--push m-aside--offcanvas-default"  >
		<!-- begin:: Page -->
		<div class="m-grid m-grid--hor m-grid--root m-page">
			<div class="m-login m-login--signin  m-login--5" id="m_login" style="background-image: url(<%= image_url "admin/app/media/img/bg/bg-3.jpg" %>);">
				



				<%= yield %>



			</div>
		</div>
		<!-- end:: Page -->
    	<!--begin::Base Scripts -->
		<%= javascript_include_tag 'application', 'admin/vendors/base/vendors.bundle.js', 'admin/demo/default/base/scripts.bundle.js', 'admin/vendors/custom/fullcalendar/fullcalendar.bundle.js', 'data-turbolinks-track': 'reload' %>


		<!--end::Page Snippets -->
	</body>
	<!-- end::Body -->
</html>
